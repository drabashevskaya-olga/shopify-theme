        $(".gallery-item").click(function(){
          $(".gallery-item").removeClass('active');
          $(this).addClass('active');
          var imgurl = $('img',this).prop('src');
          $('.gallery-img-head').attr('src',imgurl);
          if ($(this).hasClass("big-ball")) {
            $(".gallery-caption h3").text('Big Ball');
            $(".gallery-caption p").text('Perfect for large muscle groups like hips, thighs, and arms');
          } else if ($(this).hasClass("arrow-head")) {
            $(".gallery-caption h3").text('Arrow Head');
            $(".gallery-caption p").text('Provides deep tissue massage; effective for pain points');
          } else if ($(this).hasClass("flat")) {
            $(".gallery-caption h3").text('Flat');
            $(".gallery-caption p").text('A more relaxing massage, suitable for the whole body');
          } else if ($(this).hasClass("fork")) {
            $(".gallery-caption h3").text('Fork');
            $(".gallery-caption p").text('Designed to massage the neck, spine, and achilles tendon');
          } else if ($(this).hasClass("tri-head")) {
