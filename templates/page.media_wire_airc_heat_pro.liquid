                                <div class="msus--col col-half">
                                    <div class="mwus--product-original-price text-faint"><s>$129.99</s></div>
                                    <div class="mwus--product-sale-price">{{ upsell_discount_price | money }}</div>
                                </div>
                                <div class="msus--col col-half">
                                    <a href="#" class="js--upsell--trigger btn mw--btn" data-variant-id="{{ variant.id }}">Add</a>
                                </div>
                            </div>
                        </div>
                    {% else %}
                        {% for i in upsell_product_handles %}
                            {% assign product =  all_products[i] %}
                            {% assign variant = product.selected_or_first_available_variant %}
                            {% if upsell_discount_type == 'percent' %}
                                {% assign amount_off = upsell_discount_amount | times: 0.01 | times: product.price %}
                                {% assign upsell_discount_price = product.price | minus: amount_off %}
                            {% else %}
                                {% assign upsell_discount_price = product.price | minus: upsell_discount_amount %}
                            {% endif %}
