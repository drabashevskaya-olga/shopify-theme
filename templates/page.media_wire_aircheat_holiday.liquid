        </div>
        <script>

          // percent, dollar, lineitem
          const discountObj = {
            '{{ minicart_discount_code }}': { type: '{{ minicart_discount_code_type }}', amount: {{ minicart_discount_code_amount}} },
          }

          function getCookie(name) {
            var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
            return v ? v[2] : undefined;
          }

          function setCookie(name, value, days) {
            var d = new Date;
            d.setTime(d.getTime() + 24*60*60*1000*days);
            document.cookie = name + '='+value+';path=/;expires='+d.toGMTString();
          }

          function deleteCookie(name) {
            setCookie(name, '', -1);
          }

          function checkUrlParams(sParam){
            var sPageURL = decodeURIComponent(window.location.search.substring(1)),
            sURLVariables = sPageURL.split('&'), sParameterName, i;

            for (i = 0; i < sURLVariables.length; i++) {
              sParameterName = sURLVariables[i].split('=');
              if (sParameterName[0] === sParam) {
                return sParameterName[1] === undefined ? true : sParameterName[1];
              }
            }
          }

          function formatMoney(cents) {
            return '$'+(cents*0.01).toFixed(2);
          }

          function refreshMiniCart(openCart) {
            $minicart = $('.mwmc--wrapper');
            $lineitemsTarget = $minicart.find('.js--lineitems--target');
            // $lineitemsTarget.empty();

            $.getJSON('/cart.js', function(cart) {
              var runningDiscount = 0;
              var cartTotal = cart.total_price;
              var hasUpsellItem = false;

              $.each(cart.items, function(index, lineitem){
                var lineitemPricing;
                var finalLinePrice = lineitem.final_line_price
                var isUpsellItem = lineitem.properties && lineitem.properties['Special'];

                if (isUpsellItem && {{ upsell_discount_enabled }}) {
                    hasUpsellItem = true;
                }

                if (discountObj[getCookie('discount')] && discountObj[getCookie('discount')].type == 'lineitem') {
                    var lineitemDiscount

                    if (lineitem.properties && lineitem.properties['Special'] && {{ upsell_discount_enabled }}) {
                        if ('{{ upsell_discount_type }}' == 'percent') {
                            lineitemDiscount = lineitem.original_line_price * (1 - (0.01 * parseInt('{{ upsell_discount_amount }}')))
                        } else {
                            lineitemDiscount = lineitem.quantity * parseInt('{{ upsell_discount_amount }}');
                        }

                    } else {
                      lineitemDiscount = lineitem.quantity * discountObj[getCookie('discount')].amount;
                    }

                  finalLinePrice = lineitem.original_line_price - lineitemDiscount;
                  runningDiscount += lineitemDiscount;
                }

                if (finalLinePrice < lineitem.original_line_price) {
                    lineitemPricing = `<div class="mwmc--original-lineprice text-grey"><s>${formatMoney(lineitem.original_line_price)}</s></div>
                      <div class="mwmc--sale-lineprice text-green">${formatMoney(finalLinePrice)}</div>`;
                } else {
                    lineitemPricing = `<div class="mwmc--sale-lineprice text-green">${formatMoney(finalLinePrice)}</div>`;
                }

                var lineitemContent = `<div class="mw--row">
                    <div class="mwmc--img-col mw--col mw--col-6 mw--col-md-3">
                      <img src="${lineitem.image}" class="card-img-top" />
                    </div>
                    <div class="mwmc--info-col mw--col mw--col-6 mw--col-md-6">
                      <div class="mwmc--product-title">${lineitem.product_title}</div>
                      <div class="mwmc--variant-title text-grey">${lineitem.variant_title}</div>
                      <div class="mwmc--quantity--wrapper">
                        <a href="#" class="js--item-qty--trigger minus" rel="${lineitem.quantity-1}">-</a>
                        <span class="js--item-qty--target" data-variant-id="${lineitem.variant_id}">${lineitem.quantity}</span>
                        <a href="#" class="js--item-qty--trigger plus" rel="${lineitem.quantity+1}">+</a>
                      </div>
                    </div>
                    <div class="mw--col mw--col-12 mw--col-md-3 text-right">${lineitemPricing}</div>
                  </div>`;

                if ($(`.mwmc--lineitem[data-lineitem-key="${lineitem.key}"]`).length) {
                    $(`.mwmc--lineitem[data-lineitem-key="${lineitem.key}"]`).html(lineitemContent);
                } else {
                // new item
                    $lineitemsTarget.append(`<div class="mwmc--lineitem ${isUpsellItem ? 'mwmc--upsell-item' : ''}" data-lineitem-key="${lineitem.key}">${lineitemContent}</div>`);
                }


              });

              if (discountObj[getCookie('discount')] && discountObj[getCookie('discount')].type != 'lineitem') {

                if (discountObj[getCookie('discount')].type == 'percent') {
                  // percentage calc
                } else {
                  runningDiscount = discountObj[getCookie('discount')].amount;
                }

              }

              if (hasUpsellItem) {
                $('.mwmc--wrapper').addClass('has-upsell-item');
                $('.mwus--wrapper').hide();
              } else {
                $('.mwmc--wrapper').removeClass('has-upsell-item');
                $('.mwus--wrapper').show();
              }

              if (cart.items.length == 0) {
                $('.mwus--wrapper').hide();
              }

              if (getCookie('discount')) {
                $('.mwmc--wrapper .mwmc--discount-row').show();
                  $('.mwmc--wrapper .js--discount-code--target').text(getCookie('discount'));
              }

              if (runningDiscount > 0) {
                cartTotal = cart.original_total_price - runningDiscount;
              }

              if ({{ free_shipping_amount }} == 0 || cartTotal >= {{ free_shipping_amount | times: 100 }}) {
                $('.js--shipping-message--target').html('Your Cart qualifies <strong>for Free&nbsp;Shipping!</strong>');
                $('.js--shipping-row--target').show();
              } else {
                $('.js--shipping-message--target').html('Your Cart')
                $('.js--shipping-row--target').hide();
              }

              $minicart.find('.js--subtotal--target').text(formatMoney(cart.original_total_price));
              $minicart.find('.js--total--target').text(formatMoney(cartTotal));
              // $minicart.find('.js--discount--target').text('-'+formatMoney(cart.original_total_price - cart.total_price));
              // $minicart.find('.js--discount-code--target').text();

              if (openCart) {
                $('.venobox[href="#minicart"]').trigger('click');
              }

            });

          }
        </script>

        <script>
          $(document).ready(function(){
            $('body').on('click', '.js--upsell--trigger', function(event){
              event.stopImmediatePropagation();
              event.preventDefault();
              var $btn = $(this);
              var variantId = $btn.attr('data-variant-id');
              $.ajax({
                type: 'post',
                url: '/cart/add.js',
                async: false,
                data: {
                  id: variantId,
                  quantity: 1,
                  properties: {
                    'Special': ' '
                  }
                },
                dataType: 'json',
                error: function(){
                },
                success: function(data){
                  if ({{ standalone }}) {
                    ajaxCart.load();
                  } else {
                    refreshMiniCart(false);
                  }
                  if ($btn.closest('.mwus--items-row').hasClass('row--1-up')) {
                    $('.mwus--wrapper').addClass('hidden');
                  } else {
                    $parent = $btn.closest('.mwus--items-row');
                    $btn.closest('.mwus--item-col').remove();
                    if ($parent.find('.mwus--item-col').length == 0) {
                      $('.mwus--wrapper').addClass('hidden');
                    }
                  }
                  return true;
                },
                cache: false
              });
            });

            if (checkUrlParams('discount') || getCookie('discount')) {
              // console.log('has discount param')

              if (!getCookie('discount')) {
                setCookie('discount', checkUrlParams('discount').toUpperCase(), 3);
              }

              var discountCode = getCookie('discount');

              $('.mwmc--wrapper .mwmc--discount-row').show();
              $('.mwmc--wrapper .js--discount-code--target').text(discountCode);

              $('form[action="/checkout"]').attr('action','/checkout?discount='+discountCode);
              // apply to checkout forms
              // set cookie and pass to CHECKOUT URLs
            } else {
              // no code
            }

            $('body').on('click', '.js--item-qty--trigger', function(event){
              event.preventDefault();

              var lineitemKey = $(this).closest('.mwmc--lineitem').attr('data-lineitem-key');
              var $qtyInput = $(this).closest('.mwmc--quantity--wrapper').find('.js--item-qty--target');
              var variantId = parseInt($qtyInput.attr('data-variant-id'));
              var newQty = parseInt($(this).attr('rel'));


              if ($('.mwmc--wrapper').hasClass('has-upsell-item')) {
                if (!($(this).closest('.mwmc--lineitem').hasClass('mwmc--upsell-item'))) {
                    if (newQty == 0) {
                        alert('Please remove second item.');
                        return false; 
                    }
                }
              }

              if (newQty == 0) { 
                $(`.mwmc--lineitem[data-lineitem-key="${lineitemKey}"]`).remove(); 
              }

              $.ajax({
                type: 'post',
                url: '/cart/update.js',
                async: false,
                data: {
                  updates: {
                    [variantId]: newQty,
                  } 
                },
                dataType: 'json',
                error: function(){
                },
                success: function(data){
                  refreshMiniCart(false);
                  ajaxCart.load();
                  return true;
                },
                cache: false
              });

            });
            
            $.getJSON('/cart.js', function(cart) {
              if (cart.item_count != 0) {
                $.ajax({
                  type: "POST",
                  url: '/cart/clear.js',
                  data: '',
                  dataType: 'json',
                  success: function() { 
                    location.reload();
                  },
                  error: function(XMLHttpRequest, textStatus) {
                    /* error code */
                  }
                });
                return false;
              }
            });

            $('#AddToCartForm').on('submit', function(event){
              event.preventDefault();
              var formData = $(this).serialize();
            
              $.ajax({
                type: 'post',
                url: '/cart/add.js',
                async: false,
                data: formData,
                dataType: 'json',
                error: function(){
                },
                success: function(data){
                  refreshMiniCart(true);
                  return true;
                },
                cache: false
              });
            });
          
          $(window).on('load resize orientationchange', function() {
            $('.mobile-slider').each(function(){
              var $carousel = $(this);
              if ($(window).width() > 991) {
                if ($carousel.hasClass('slick-initialized')) {
                  $carousel.slick('unslick');
                }
              }
              else {
                if (!$carousel.hasClass('slick-initialized')) {
                  $carousel.slick({
                    dots: true,
                    centerMode: true,
                    centerPadding: '40px',
                    slidesToShow: 1,
                    infinite: true,
                    autoplay: false,
                    autoplaySpeed: 9000,
                    adaptiveHeight: true,
                    arrows: true,
                    nextArrow: '<i class="fa fa-arrow-right large_clear"></i>',
                    prevArrow: '<i class="fa fa-arrow-left large_clear"></i>',
                  });
                }
              }
            });
          });
          
          /* FAQ */
          var allPanels = $('#faq dd').hide();

          $('#faq dt').click(function() {
            allPanels.slideUp();
            $(this).next().slideDown();
            return false;
          });

          /* YOUTUBE */
          $(".youtube").click(function(ev){
            $(this).find('.iframewrapper').show();
            $('iframe',this)[0].src += "?autoplay=1";
          });
            
          var countDownDate = new Date("{{ settings.mw_holiday_timer }} 23:59:59").getTime();
                                       
          // Update the count down every 1 second
          var x = setInterval(function() {

            // Get today's date and time
            var now = new Date().getTime();

            // Find the distance between now and the count down date
            var distance = countDownDate - now;

            // Time calculations for days, hours, minutes and seconds
            var days = Math.floor(distance / (1000 * 60 * 60 * 24));
            var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
            var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
            var seconds = Math.floor((distance % (1000 * 60)) / 1000);

            // Display the result in the element with id="demo"
            document.getElementById("holiday-countdown").innerHTML = 
              "<div>" + days + "<span>Days</span></div>" 
            + "<div>" + hours + "<span>Hours</span></div>"
            + "<div>" + minutes + "<span>Minutes</span></div>" 
            + "<div>" + seconds + "<span>Seconds</span></div>";

            // If the count down is finished, write some text
            if (distance < 0) {
              clearInterval(x);
              document.getElementById("holiday-countdown").innerHTML = "EXPIRED";
            }
          }, 1000);

          }); 
        </script>
      
    </div>




