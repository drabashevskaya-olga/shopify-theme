                                <div class="mwus--product-row">
                                    <div class="mwus--img-col">
                                        <img src="{{ product.featured_image | img_url: '200x' }}" alt="{{ product.title }}">
                                    </div>
                                    <div class="mwus--info-col">
                                        <div class="mwus--product-title ">{{ product.title }}</div>
                                        <div class="mwus--variant-title text-grey">{{ variant.title }}</div>
                                        <div class="mwus--product-original-price text-grey"><s>{{ product.price | money }}</s></div>
                                        <div class="mwus--product-sale-price text-green">{{ upsell_discount_price | money }}</div>
                                    </div>
