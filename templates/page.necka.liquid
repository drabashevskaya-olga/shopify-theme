              if ($('.mwmc--wrapper').hasClass('has-upsell-item')) {
                if (!($(this).closest('.mwmc--lineitem').hasClass('mwmc--upsell-item'))) {
                    if (newQty == 0) {
                        alert('Please remove second item.');
                        return false; 
                    }
                }
              }

              if (newQty == 0) { 
                $(`.mwmc--lineitem[data-lineitem-key="${lineitemKey}"]`).remove(); 
              }

              $.ajax({
                type: 'post',
                url: '/cart/update.js',
                async: false,
                data: {
                  updates: {
                    [variantId]: newQty,
                  } 
                },
                dataType: 'json',
                error: function(){
                },
                success: function(data){
                  refreshMiniCart(false);
                  ajaxCart.load();
                  return true;
                },
                cache: false
              });

            });
            
            $.getJSON('/cart.js', function(cart) {
              if (cart.item_count != 0) {
                $.ajax({
                  type: "POST",
                  url: '/cart/clear.js',
                  data: '',
                  dataType: 'json',
                  success: function() { 
                    location.reload();
                  },
                  error: function(XMLHttpRequest, textStatus) {
                    /* error code */
                  }
                });
                return false;
              }
            });

            $('#AddToCartForm').on('submit', function(event){
              event.preventDefault();
              var formData = $(this).serialize();
            
              $.ajax({
                type: 'post',
                url: '/cart/add.js',
                async: false,
                data: formData,
                dataType: 'json',
                error: function(){
                },
                success: function(data){
                  refreshMiniCart(true);
                  return true;
                },
                cache: false
              });
            });
          
