        <script>

          // percent, dollar, lineitem
          const discountObj = {
            '{{ minicart_discount_code }}': { type: '{{ minicart_discount_code_type }}', amount: {{ minicart_discount_code_amount}} },
          }

          function getCookie(name) {
            var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
            return v ? v[2] : undefined;
          }

          function setCookie(name, value, days) {
            var d = new Date;
            d.setTime(d.getTime() + 24*60*60*1000*days);
            document.cookie = name + '='+value+';path=/;expires='+d.toGMTString();
          }

          function deleteCookie(name) {
            setCookie(name, '', -1);
          }

          function checkUrlParams(sParam){
            var sPageURL = decodeURIComponent(window.location.search.substring(1)),
            sURLVariables = sPageURL.split('&'), sParameterName, i;

            for (i = 0; i < sURLVariables.length; i++) {
              sParameterName = sURLVariables[i].split('=');
              if (sParameterName[0] === sParam) {
                return sParameterName[1] === undefined ? true : sParameterName[1];
              }
            }
          }

          function formatMoney(cents) {
            return '$'+(cents*0.01).toFixed(2);
          }
var Shopify = Shopify || {};
// ---------------------------------------------------------------------------
// Money format handler
// ---------------------------------------------------------------------------
Shopify.money_format = "{{ shop.money_format }}";
Shopify.formatMoney = function(cents, format) {
  if (typeof cents == 'string') { cents = cents.replace('.',''); }
  var value = '';
  var placeholderRegex = /\{\{\s*(\w+)\s*\}\}/;
  var formatString = (format || this.money_format);

  function defaultOption(opt, def) {
     return (typeof opt == 'undefined' ? def : opt);
  }

  function formatWithDelimiters(number, precision, thousands, decimal) {
    precision = defaultOption(precision, 2);
    thousands = defaultOption(thousands, ',');
    decimal   = defaultOption(decimal, '.');

    if (isNaN(number) || number == null) { return 0; }

    number = (number/100.0).toFixed(precision);

    var parts   = number.split('.'),
        dollars = parts[0].replace(/(\d)(?=(\d\d\d)+(?!\d))/g, '$1' + thousands),
        cents   = parts[1] ? (decimal + parts[1]) : '';

    return dollars + cents;
  }

  switch(formatString.match(placeholderRegex)[1]) {
    case 'amount':
      value = formatWithDelimiters(cents, 2);
      break;
    case 'amount_no_decimals':
      value = formatWithDelimiters(cents, 0);
      break;
    case 'amount_with_comma_separator':
      value = formatWithDelimiters(cents, 2, '.', ',');
      break;
    case 'amount_no_decimals_with_comma_separator':
      value = formatWithDelimiters(cents, 0, '.', ',');
      break;
  }

  return formatString.replace(placeholderRegex, value);
};

          function refreshMiniCart(openCart) {
            $minicart = $('.mwmc--wrapper');
            $lineitemsTarget = $minicart.find('.js--lineitems--target');
            // $lineitemsTarget.empty();

            $.getJSON('/cart.js', function(cart) {
              var runningDiscount = 0;
              var cartTotal = cart.total_price;
              var hasUpsellItem = false;
console.log(cart);
              $.each(cart.items, function(index, lineitem){
                var lineitemPricing;
                var finalLinePrice = lineitem.final_line_price
                var isUpsellItem = lineitem.properties && lineitem.properties['Special'];

                if (isUpsellItem && {{ upsell_discount_enabled }}) {
                    hasUpsellItem = true;
                }

                if (discountObj[getCookie('discount')] && discountObj[getCookie('discount')].type == 'lineitem') {
                    var lineitemDiscount

                    if (lineitem.properties && lineitem.properties['Special'] && {{ upsell_discount_enabled }}) {
                        if ('{{ upsell_discount_type }}' == 'percent') {
                            lineitemDiscount = lineitem.original_line_price * (1 - (0.01 * parseInt('{{ upsell_discount_amount }}')))
                        } else {
                            lineitemDiscount = lineitem.quantity * parseInt('{{ upsell_discount_amount }}');
                        }

                    } else {
                      lineitemDiscount = lineitem.quantity * discountObj[getCookie('discount')].amount;
                    }

                  finalLinePrice = lineitem.original_line_price - lineitemDiscount;
                  runningDiscount += lineitemDiscount;
                }

                if (finalLinePrice < lineitem.original_line_price) {
                    lineitemPricing = `<div class="mwmc--original-lineprice text-grey"><s>${Shopify.formatMoney(lineitem.original_line_price)}</s></div>
                      <div class="mwmc--sale-lineprice text-green">${Shopify.formatMoney(finalLinePrice)}</div>`;
                } else {
                    lineitemPricing = `<div class="mwmc--sale-lineprice text-green">${Shopify.formatMoney(finalLinePrice)}</div>`;
                }

                var lineitemContent = `<div class="mw--row">
                    <div class="mwmc--img-col mw--col mw--col-6 mw--col-md-3">
                      <img src="${lineitem.image}" class="card-img-top" />
                    </div>
                    <div class="mwmc--info-col mw--col mw--col-6 mw--col-md-6">
                      <div class="mwmc--product-title">${lineitem.product_title}</div>
                      <div class="mwmc--variant-title text-grey">${lineitem.variant_title}</div>
                      <div class="mwmc--quantity--wrapper">
                        <a href="#" class="js--item-qty--trigger minus" rel="${lineitem.quantity-1}">-</a>
                        <span class="js--item-qty--target" data-variant-id="${lineitem.variant_id}">${lineitem.quantity}</span>
                        <a href="#" class="js--item-qty--trigger plus" rel="${lineitem.quantity+1}">+</a>
                      </div>
                    </div>
                    <div class="mw--col mw--col-12 mw--col-md-3 text-right md-text-left cstm-rigth-sct">${lineitemPricing}</div>
                  </div>`;

                if ($(`.mwmc--lineitem[data-lineitem-key="${lineitem.key}"]`).length) {
                    $(`.mwmc--lineitem[data-lineitem-key="${lineitem.key}"]`).html(lineitemContent);
                } else {
                // new item
                    $lineitemsTarget.append(`<div class="mwmc--lineitem ${isUpsellItem ? 'mwmc--upsell-item' : ''}" data-lineitem-key="${lineitem.key}">${lineitemContent}</div>`);
                }


              });

              if (discountObj[getCookie('discount')] && discountObj[getCookie('discount')].type != 'lineitem') {

                if (discountObj[getCookie('discount')].type == 'percent') {
                  // percentage calc
                } else {
                  runningDiscount = discountObj[getCookie('discount')].amount;
                }

              }

              if (hasUpsellItem) {
                $('.mwmc--wrapper').addClass('has-upsell-item');
                $('.mwus--wrapper').hide();
              } else {
                $('.mwmc--wrapper').removeClass('has-upsell-item');
                $('.mwus--wrapper').show();
              }

              if (cart.items.length == 0) {
                $('.mwus--wrapper').hide();
              }

              if (getCookie('discount')) {
                $('.mwmc--wrapper .mwmc--discount-row').show();
                  $('.mwmc--wrapper .js--discount-code--target').text(getCookie('discount'));
              }

              if (runningDiscount > 0) {
                cartTotal = cart.original_total_price - runningDiscount;
              }

              if ({{ free_shipping_amount }} == 0 || cartTotal >= {{ free_shipping_amount | times: 100 }}) {
                $('.js--shipping-message--target').html('Your Cart qualifies <strong>for Free&nbsp;Shipping!</strong>');
                $('.js--shipping-row--target').show();
              } else {
                $('.js--shipping-message--target').html('Your Cart')
                $('.js--shipping-row--target').hide();
              }

              $minicart.find('.js--subtotal--target').text(Shopify.formatMoney(cart.original_total_price));
              $minicart.find('.js--total--target').text(Shopify.formatMoney(cartTotal));
              // $minicart.find('.js--discount--target').text('-'+Shopify.formatMoney(cart.original_total_price - cart.total_price));
              // $minicart.find('.js--discount-code--target').text();

              if (openCart) {
                $('.venobox[href="#minicart"]').trigger('click');
              }

            });

          }
