.cover {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #0006;
  z-index: 99;
}

.cover .cover-inner {
  width: 802px;
  height: 80%;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.cover .cover-content-main {
  background: #fff;
  height: 100%;
  padding:30px 17px 30px 30px;
  border-radius: 20px;
  position:relative;
}
.cover .contents-inner {
    padding-bottom: 50px;
}
.cover .cover-content-main:after {
  content: '';
  position: absolute;
  background-image: linear-gradient(rgba(255,255,255,.3), #fff); 
  width: 100%;
  height:23%;
  bottom: 0;
  left: 0;
  border-radius:20px;
} 
.cover .cover-content-main a.button.compare_now {
  position: relative;
  z-index: 2;
}
main#MainContent {
  overflow-x: hidden;
}
.cover .cover-content-main .contents {
  overflow-y: auto;
  height: 100%;
  width: 100%;
  padding-right: 20px;

}
.cover .cover-content-main h1 {
  margin: 0;
  font-size: 20px;
  line-height: 26px;
  font-weight: 600;
  color: #000;
}
.cover .cover-content-main .compare_collection {
  height: 48px;
  background: #F7F7F7;
  display: flex;
  align-items: center;
  padding: 10px 20px;
  border-radius: 7px;
  margin: 0 0 33px 0;
}
.cover .cover-content-main .compare_list li {
  display: flex;
  width: 100%;
  text-align: left;
  padding: 0;
  align-items: center;
  margin: 0 0 20px 0;
}
.cover .cover-content-main ul.compare_list {
  margin: 0 0 50px;
  display: inline-block;
  width: 100%;
  padding:0;
}
.cover .cover-content-main .compare_list li:last-child {
  margin:0 0 0 0; 
}
.custom-input {
  position: relative;
  width: 24px;
  height: 24px;
}
.custom-input span.checkmark {
  position: absolute;
  width: 24px;
  height: 24px;
  background: #fff;
  left: 0;
  top: 0;
  border: 1px solid #2ECC87;
  border-radius: 2px;
}
.custom-input input {
  position: absolute;
  margin: 0;
  z-index: 9;
  width: 100%;
  height: 100%;
  opacity: 0;
  cursor:pointer;
}
.custom-input span.checkmark:after {
  content: '';
  position: absolute;
  left: 8px;
  top: 3px;
  width: 7px;
  height: 13px;
  border: solid white;
  border-width: 0 2px 2px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
  display:none;
}
/* .custom-input input:hover +span.checkmark {
  background: #2ECC87;
} */
.custom-input input:checked + span.checkmark:after{
  display:block;
}
.custom-input input:checked + span.checkmark{
  background: #2ECC87;
}
.cover .cover-inner span.close {
  position: absolute;
  bottom: 103%;
  right: -50px;
  cursor:pointer;
}
.cover .cover-inner span.close i.fa.fa-times {
  color: #fff;
}
.cover .cover-content-main .compare_list li img {
  width: 53px;
  height: 53px;
  object-fit: cover;
  border-radius: 8px;
  display: block;
  background: #eeeeee;
  margin:0;
}
.cover .cover-content-main .compare_list li .compare-image {
  padding: 0 23px;
}
.cover .cover-content-main .compare_list li .comapre-title {
  font-size: 16px;
  line-height: 22px;
  font-weight: 600;
  text-transform: uppercase;
  margin: 0 0 2px 0;
}
.cover .cover-content-main .compare_list li p {
  margin: 0;
  font-size: 16px;
  line-height: 18px;
  color: #6E6E73;
  font-weight: 500;
}

.cover .cover-content-main .compare_list li .compare-inner {
  width: calc(100% - 123px);
}
.cover ::-webkit-scrollbar {
  width: 7px;
}

.cover ::-webkit-scrollbar-track {
  background: #f1f1f1; 
  border-radius:4px;
}

.cover ::-webkit-scrollbar-thumb {
  background: #8C8C8C; 
  border-radius:4px;
}
