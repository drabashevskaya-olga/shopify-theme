.community-warpper-data {
    display: flex;
    margin-bottom: 20px;
}
.community-warpper-content {
    padding: 70px 90px;
    width: 65%;
    background-color: #f7f7f7;
    border-radius: 25px;
    margin-right: 30px;
}
.page-community .video-section-community .community-warpper-content {
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.community-warpper-content h2 {
    font-size: 48px;
    line-height: 56px;
    margin-bottom: 20px;
    margin-top: 0;
}
.community-warpper-content p {
    color: #6e6e73;
    font-size: 26px;
    line-height: 33.8px;
    font-weight: 500;
    letter-spacing: -2px;
    margin: 0;
}

.community-warpper-image {
    width: 35%;
  position:relative;
}
.community-warpper-image img {
    width: 100%;
    height: auto;
    display: block;
}
.community-video-icon {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}
.community-video-icon img {
    width: 140px;
}

.community-warpper-content h3 {
    margin: 0px 0 20px;
    font-size: 36px;
    line-height: 100%;
}