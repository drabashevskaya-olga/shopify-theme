<section class="dark_section">
        <div class="logo-row">
            <div class="container">
              {% if section.settings.heading != blank %}
                <hr class="hr-text logo_heading" data-content="{{ section.settings.heading }}">
              {% endif %}
                <div class="logo_main">
                  {% for block in section.blocks %}
                    {% if block.settings.logo_image != blank %}
                    <div class="logo_main_inner">
                        <img src="{{ block.settings.logo_image | img_url:'master' }}" alt="logo" />
                    </div>
                    {% endif %}
                  {% endfor %}
                </div>
            </div>
        </div>
    </section>

{% schema %}
  {
    "name": "Featured Section",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading"
      }
    ],
    "blocks": [
     {
       "name": "Logo",
       "type": "logo",
       "settings": [
         {
           "type": "image_picker",
           "id": "logo_image",
           "label": "Logo"
         }
       ]
     }
  ]
  }
{% endschema %}

