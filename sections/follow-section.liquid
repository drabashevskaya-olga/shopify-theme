{% schema %}
  {
    "name": "Follow Section",
      "settings": [
  {
  "type": "text",
  "id": "title",
  "default": "Heading",
  "label": "Heading"
  },
  {
  "type": "text",
  "id": "sub_heading",
  "default": "Sub Heading",
  "label": "Sub Heading"
  },
  {
  "type": "text",
  "id": "button_label",
  "default": "View All",
  "label": "Button Label"
  },
  {
  "type": "url",
  "id": "button_url",
  "label": "Button Url"
  },
  {
  "type": "url",
  "id": "youtube_url",
  "label": "Youtube Url"
  },
  {
  "type": "url",
  "id": "facebook_url",
  "label": "Facebook Url"
  },
  {
  "type": "url",
  "id": "instagram_url",
  "label": "Instagram Url"
  },
  {
  "type": "url",
  "id": "twitter_url",
  "label": "Twitter Url"
  }
	],
    "presets": [
    {
    "name": "Follow-section",
    "category": "Follow"
    }
    ]
  }
{% endschema %}

